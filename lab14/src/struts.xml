<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<!-- 实时更新到服务器 -->
 <constant name="struts.devMode" value="true" />
	<package name="lab14" extends="struts-default">
		<action name="*-*" class="action.{1}Action" method="{2}">
			<result>/WEB-APP/{1}{2}-success.jsp</result>
			<result name="error">/WEB-APP/{1}{2}-error.jsp</result>
			<result name="push">/WEB-APP/addNews.jsp</result>
			<result name="alltype">/WEB-APP/seeCategorys.jsp</result>
			<result name="findbytype">/WEB-APP/newsfind-success.jsp</result>
		</action>
	</package>
</struts>    
